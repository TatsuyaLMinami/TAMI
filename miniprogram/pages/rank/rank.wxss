.rank {
    display: flex;
    flex-wrap: wrap;
    width: 90%;
    
}
.ellipsis{
    text-overflow :ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.ellipsis:hover{
    overflow: visible;
}
.table {
    border: 0px solid darkgray;
    width:100%;
}

.tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 2.5rem;
    align-items: center;
}

.td {
    width: 40%;
    justify-content: center;
    text-align: center;
}

.bg-w {
    background: snow;
}

.bg-g {
    background: #E6F3F9;
}

.th {
    width: 40%;
    justify-content: center;
    background: #3366FF;
    color: #fff;
    display: flex;
    height: 2.5rem;
    align-items: center;
}